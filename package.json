{
  "name": "prayer-app-iccec",
  "version": "1.0.0",
  "description": "Cenacle (Upper Room) - Multi-language prayer and daily readings app",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "build:ios": "eas build --platform ios",
    "build:android": "eas build --platform android",
    "build:all": "eas build --platform all",
    "build:ios:production": "eas build --platform ios --profile production",
    "build:android:production": "eas build --platform android --profile production",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "check-production": "node check-production-ready.js"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/stack": "^6.4.1",
    "date-fns": "^4.1.0",
    "expo": "~54.0.33",
    "expo-dev-client": "~6.0.20",
    "expo-font": "~14.0.9",
    "expo-localization": "~17.0.8",
    "expo-status-bar": "~3.0.9",
    "fbemitter": "^3.0.0",
    "i18n-js": "^4.4.3",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-paper": "^5.12.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-vector-icons": "^10.2.0",
    "react-native-webview": "13.15.0",
    "typescript": "~5.9.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/jest": "^29.4.0",
    "@types/react": "~19.1.0",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-preset-expo": "^54.0.4",
    "eslint": "^8.57.0",
    "jest": "^29.2.1",
    "prettier": "^3.3.0"
  },
  "keywords": [
    "expo",
    "react-native",
    "typescript",
    "prayer",
    "iccec",
    "multilingual",
    "offline-first"
  ],
  "author": "ICCEC Europe",
  "license": "MIT",
  "private": true
}
